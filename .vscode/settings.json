{
    //"explorer.excludeGitIgnore": true,
    "C_Cpp.default.includePath": [
        "/usr/include/**",
        "/usr/local/include/**",
        "./lib/LiveContainer",
        "./lib/fishhook",
        "./lib/utils",
    ],
    "clang.objcflags": [
        "-I./lib/LiveContainer",
        "-I$./lib/fishhook",
        "-I$./lib/utils",
    ],
    "C_Cpp.default.systemIncludePath": [
        "lib/utils",
        "lib/fishhook",
        "lib/LiveContainer",
    ],
    "files.associations": {
        "*.m": "objective-c"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".keys": true,
        ".secrets": true,
    }
}